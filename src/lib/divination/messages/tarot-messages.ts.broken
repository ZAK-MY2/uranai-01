// タロットメッセージデータベース
// 78枚のカードごとに豊富なメッセージパターンを提供
// 著名タロット占術師の研究に基づく商用グレード品質

export interface TarotCardMessage {
  // カード個別の基本解釈
  uprightInterpretations: string[];    // 正位置の解釈（10パターン以上）
  reversedInterpretations: string[];   // 逆位置の解釈（10パターン以上）
  
  // 位置別の解釈（スプレッド対応）
  positionInterpretations: {
    past: string[];                    // 過去の位置での解釈
    present: string[];                 // 現在の位置での解釈
    future: string[];                  // 未来の位置での解釈
    advice: string[];                  // アドバイスの位置での解釈
    obstacle: string[];                // 障害の位置での解釈
    outcome: string[];                 // 結果の位置での解釈
    innerSelf: string[];               // 内面の位置での解釈
    environment: string[];             // 環境の位置での解釈
    hopes: string[];                   // 希望の位置での解釈
    fears: string[];                   // 恐れの位置での解釈
  };
  
  // 質問カテゴリ別の解釈
  categoryInterpretations: {
    love: string[];                    // 恋愛に関する解釈
    career: string[];                  // 仕事に関する解釈
    health: string[];                  // 健康に関する解釈
    finance: string[];                 // 金運に関する解釈
    spiritual: string[];               // 精神面に関する解釈
    general: string[];                 // 総合運に関する解釈
  };
  
  // 時期別メッセージ
  timingMessages: {
    morning: string[];                 // 朝の占いメッセージ
    afternoon: string[];               // 昼の占いメッセージ
    evening: string[];                 // 夜の占いメッセージ
    weekly: string[];                  // 週間メッセージ
    monthly: string[];                 // 月間メッセージ
  };
  
  // 組み合わせ解釈（他のカードとの相性）
  combinations: {
    withMajorArcana: Record<string, string>;  // 大アルカナとの組み合わせ
    withSameSuit: Record<string, string>;     // 同じスートとの組み合わせ
    withCourtCards: Record<string, string>;   // コートカードとの組み合わせ
  };
  
  // 詩的表現（石井ゆかり風）
  poeticExpressions: string[];
  
  // 心理学的解釈（鏡リュウジ風）
  psychologicalInsights: string[];
  
  // 実践的アドバイス（yuji風）
  practicalAdvice: string[];
}

